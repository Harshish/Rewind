{
    "manifest_version": 2,
    "version": "1.0",
    "name": "Rewind",
    "description": "Fasttrack to your previous tabs",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "background": {
        "scripts": ["jquery-3.2.1.min.js", "eventPage.js"],
        "persistent": false
    },
    "permissions": [
        "history",
        "storage",
        "tabs",
        "notifications"
    ]
}